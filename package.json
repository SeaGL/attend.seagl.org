        "@babel/register": "^7.12.10",
        "@babel/runtime": "^7.12.5",
        "@casualbot/jest-sonar-reporter": "^2.2.5",
        "@principalstudio/html-webpack-inject-preload": "^1.2.7",
        "@sentry/webpack-plugin": "^2.0.0",
